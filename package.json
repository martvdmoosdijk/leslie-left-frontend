{
  "name": "leslie-left-frontend",
  "private": true,
  "description": "The main website for the band called Leslie Left",
  "version": "1.0.0",
  "author": "Mart van de Moosdijk martvandemoosdijk@gmail.com>",
  "keywords": [
    "gatsby",
    "leslie-left"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "develop-mobile": "gatsby develop -H 0.0.0.0 -p 8000",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "serve-mobile": "gatsby serve -H 0.0.0.0 -p 9000",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "deploy": "rm -rf .cache && rm -rf public && rm -rf docs && gatsby build && echo www.leslieleft.com > public/CNAME && gh-pages -d public",
    "deploy-windows": "gatsby build && echo www.leslieleft.com > public/CNAME && gh-pages -d public"
  },
  "dependencies": {
    "animejs": "^3.1.0",
    "babel-plugin-styled-components": "^2.0.6",
    "gatsby": "^4.10.3",
    "gatsby-plugin-google-analytics": "^4.10.0",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-react-helmet": "^5.10.0",
    "gatsby-plugin-styled-components": "^5.10.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-div-100vh": "^0.7.0",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "styled-components": "^5.0.0"
  },
  "devDependencies": {
    "eslint": "^8.12.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "gh-pages": "^3.2.3"
  }
}
