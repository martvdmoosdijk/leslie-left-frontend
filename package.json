{
  "name": "leslie-left-frontend",
  "private": true,
  "description": "The main website for the band called Leslie Left",
  "version": "1.0.0",
  "author": "Mart van de Moosdijk martvandemoosdijk@gmail.com>",
  "keywords": [
    "gatsby",
    "leslie-left"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "develop-mobile": "gatsby develop -H 0.0.0.0 -p 8000",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "serve-mobile": "gatsby serve -H 0.0.0.0 -p 9000",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "deploy": "rm -rf .cache && rm -rf public && rm -rf docs && gatsby build && echo www.leslieleft.com > public/CNAME && gh-pages -d public"
  },
  "dependencies": {
    "animejs": "^3.1.0",
    "babel-plugin-styled-components": "^1.10.6",
    "gatsby": "^2.17.10",
    "gatsby-plugin-google-analytics": "^2.1.24",
    "gatsby-plugin-prefetch-google-fonts": "^1.4.3",
    "gatsby-plugin-react-helmet": "^3.1.13",
    "gatsby-plugin-styled-components": "^3.1.11",
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-div-100vh": "^0.3.8",
    "react-dom": "^16.11.0",
    "react-helmet": "^5.2.1",
    "styled-components": "^5.0.0"
  },
  "devDependencies": {
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "gh-pages": "^2.1.1"
  }
